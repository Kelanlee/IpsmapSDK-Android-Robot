<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <!--<FrameLayout-->
        <!--android:visibility="gone"-->
        <!--android:id="@+id/fl_content"-->
        <!--android:layout_width="913dp"-->
        <!--android:layout_height="493dp"-->
        <!--android:layout_marginRight="20dp"-->
        <!--android:layout_marginBottom="142dp"-->
        <!--android:layout_alignParentBottom="true"-->
        <!--android:layout_alignParentRight="true"/>-->

    <FrameLayout
        android:visibility="gone"
        android:id="@+id/fl_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:id="@+id/btn_start_sdk_activity1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_centerHorizontal="true"
            android:text="方式1"
            android:layout_alignParentBottom="true"/>
        <Button
            android:id="@+id/btn_start_sdk_activity2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_centerHorizontal="true"
            android:text="方式2"
            android:layout_alignParentBottom="true"/>
        <Button
            android:id="@+id/btn_start_sdk_activity3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_centerHorizontal="true"
            android:text="方式3"
            android:layout_alignParentBottom="true"/>
        <Button
            android:id="@+id/btn_start_sdk_activity4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_centerHorizontal="true"
            android:text="方式4"
            android:layout_alignParentBottom="true"/>


        <Button
            android:id="@+id/btn_hint_map"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="8dp"
            android:text="隐藏地图"
            />
       <Button
           android:id="@+id/ipsmap_hide_home"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:layout_alignParentBottom="true"
           android:layout_centerHorizontal="true"
           android:layout_marginBottom="8dp"
            android:text="隐藏home回退键"
            />
        <Button
            android:id="@+id/btn_targetid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="8dp"
            android:text="传递目的地参数(界面显示的时候)"
            />

        <Button
            android:id="@+id/btn_hint_search"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="8dp"
            android:text="隐藏搜索"
            />

        <Button
            android:id="@+id/btn_set_key"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="8dp"
            android:text="设置秘钥"
            />

    </LinearLayout>



</LinearLayout>
